{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "configure": [
          { "Name": "Console" }
        ]
      }
    },
    "Enrich": ["WithMachineName", "FromLogContext"]
  },

  "IssLocationServiceSettings": {
    "Name": "Iss Location Service",
    "ServiceDocumentationUrl": "https://localhost/IssLocationService/api/v1/documentation/",
    "Description": "Iss Location Service",
    "ContactName": "Sal Zaki",
    "ContactEmail": "salzaki@hotmail.com",
    "TermOfServiceUrl": "https://localhost/IssLocationService/api/v1/terms/",
    "LicenseName": "Enterprise",
    "LicenseUrl": "https://localhost/IssLocationService/api/v1/license/",
    "LocationApiUrl": "http://api.open-notify.org/iss-now.json"
  }
}